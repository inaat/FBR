
{
    "name": "fbr/digital-invoicing",
    "description": "Comprehensive Laravel package for Pakistan's Federal Board of Revenue (FBR) Digital Invoicing System API v1.12 integration with support for all 28 sandbox scenarios, reference data APIs, and complete invoice management",
    "keywords": [
        "laravel", 
        "pakistan", 
        "fbr", 
        "digital-invoicing", 
        "sales-tax", 
        "invoice", 
        "api", 
        "integration", 
        "tax-management", 
        "e-invoicing",
        "government-api",
        "pakistan-tax",
        "sales-tax-api",
        "fbr-api"
    ],
    "homepage": "https://github.com/inaat/fbr-digital-invoicing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Inayat Ullah",
            "email": "inayatullahkks@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fbr\\DigitalInvoicing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fbr\\DigitalInvoicing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fbr\\DigitalInvoicing\\FbrDigitalInvoicingServiceProvider"
            ],
            "aliases": {
                "FbrDigitalInvoicing": "Fbr\\DigitalInvoicing\\Facades\\FbrDigitalInvoicing"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "fbr-test": "php tests/test-simple-package.php"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

